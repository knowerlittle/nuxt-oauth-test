{
  "name": "nuxt-oauth",
  "version": "2.1.1",
  "description": "OAuth module for your Nuxt applications",
  "main": "index.js",
  "repository": "https://github.com/SohoHouse/nuxt-oauth",
  "author": "samtgarson@gmail.com",
  "license": "MIT",
  "scripts": {
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "jest ./test/unit",
    "test:e2e": "NODE_ENV=test jest ./test/e2e/index.js",
    "test:lint": "eslint --ignore-path .gitignore .",
    "dev": "NODE_ENV=development nuxt -c ./test/e2e/fixture/nuxt.config.js"
  },
  "dependencies": {
    "Base64": "^1.0.1",
    "client-oauth2": "^4.1.0",
    "client-sessions": "^0.8.0",
    "qs": "^6.5.0",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "@sohohouse/eslint-config": "^0.0.4",
    "babel-eslint": "^7.2.3",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "dotenv": "^6.0.0",
    "eslint": "^4.5.0",
    "jest": "^20.0.4",
    "moment": "^2.18.1",
    "nuxt": "^1.4.1",
    "request-promise-native": "^1.0.5",
    "vuex": "^3.0.0"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "babel-plugin-dynamic-import-node"
    ]
  },
  "jest": {
    "clearMocks": true,
    "testMatch": [
      "<rootDir>/test/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/test/e2e/fixture/"
    ],
    "moduleNameMapper": {
      "^@/(.+)": "<rootDir>/lib/$1"
    },
    "testEnvironment": "node"
  },
  "eslintConfig": {
    "extends": [
      "@sohohouse"
    ]
  }
}
